@App:name("testapp")
@App:description("App for CEP")

@source(type = "http", receiver.url = "http://ec2-35/testreceiver", 
@map(type = "json"))
define stream testreceiverStream (temperature float, humidity float);

@sink(type = "log", prefix = "LOGGER")
 define stream testforwarderStream (temperature float, humidity float);

@info(name = "testquery")
from testreceiverStream select * insert into testforwarderStream;